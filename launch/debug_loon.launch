<launch>
  <arg name="data_directory" default="/home/lukas/Documents/1908_ExperimentsLukas" />
  <arg name="dir" default="8" />
  <arg name="rate" default="1"/>
	<!-- drift: 3,(5), strange yaw: 1,2,5,8 , not flying: 6,7 -->


